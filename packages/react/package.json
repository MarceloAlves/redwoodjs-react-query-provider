  },
  "devDependencies": {
    "@redwoodjs/auth": "^0.23.0",
    "@redwoodjs/web": "^0.23.0",
    "@redwoodjs/core": "^0.23.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "graphql": "^15.4.0",
    "react-query": "^3.5.16",
    "react": "^17.0.1",
